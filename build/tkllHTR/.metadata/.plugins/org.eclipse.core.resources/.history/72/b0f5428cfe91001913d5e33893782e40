#ifndef SRC_MATRIX_H_
#define SRC_MATRIX_H_

#include <stdlib.h>

typedef struct Matrix Matrix;
struct Matrix {
	int row = 0;
	int col = 0;
	double** at = NULL;
};

Matrix* new_matrix(int row, int col);
void del_matrix(double** a, int row);
double** add(double** a, double** b, int ra, int ca, int rb, int cb);
double** mul(double** a, double** b, int ra, int ca, int rb, int cb);




#endif /* SRC_MATRIX_H_ */
