#include "matrix.h"

Matrix weight(int row, int col, int id){
	Matrix w = new_matrix(row, col);
	w.at[0] = (double[2]){1, 2};
	return w;
}

Matrix bias(int row, int col, int id){
	Matrix b = new_matrix(row, col);
	return b;
}
